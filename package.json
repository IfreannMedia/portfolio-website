    "autoprefixer": "^10.4.2",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "typescript": "~4.5.2"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.1.2"
  }
}
