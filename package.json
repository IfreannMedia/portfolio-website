{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-prod": "ng build --configuration production && npm run prefixcss",
    "heroku-postbuild": "ng build --aot --prod",
    "prefixcss": "postcss -u autoprefixer --autoprefixer.browsers 'last 2 versions' -r dist/**/*.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "express": "^4.17.3",
    "ngx-owl-carousel-o": "^7.0.1",
    "path": "^0.12.7",
    "rxjs": "~7.5.0",
    "skeleton-screen-css": "^1.0.1",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.0",
